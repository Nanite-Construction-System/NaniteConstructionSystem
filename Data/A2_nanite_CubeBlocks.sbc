        <Model BuildPercentUpperBound="0.25" File="Models\Cubes\Large\NaniteControlFacility_Constr1.mwm" />
        <Model BuildPercentUpperBound="0.50" File="Models\Cubes\Large\NaniteControlFacility_Constr2.mwm" />
        <Model BuildPercentUpperBound="0.75" File="Models\Cubes\Large\NaniteControlFacility_Constr3.mwm" />
      </BuildProgressModels> 
	  <InventorySize>
        <X>1</X>
        <Y>1</Y>
        <Z>1</Z>
      </InventorySize>   
